<h1>Login</h1>

<.form for={@changeset} action={~p"/auth/log-user-in"} method="post" :let={f}>
  <div class="form-group">
    <.input
      field={f[:email]}
      placeholder="Title"
      class="form-control"
    />
  </div>
  <div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
</.form>
